{
  "version": 2,
  "builds": [
    {
      "src": "server.js", 
      "use": "@vercel/node"
    },
    {
      "src": "FrontEnd_Test/dist/**", 
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)", 
      "dest": "server.js"
    },
    {
      "src": "/(.*)", 
      "dest": "FrontEnd_Test/dist/index.html"
    }
  ]
}
